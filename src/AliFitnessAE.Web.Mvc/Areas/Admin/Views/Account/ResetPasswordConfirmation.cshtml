@model AliFitnessAE.Web.Models.Admin.Account.ResetPasswordRequest
@{
    ViewBag.Title = L("ResetPasswordConfirmation");
}
<div class="card">
    <div class="card-body">
        <div class="alert alert-default-light" role="alert">
            <h4>@L("ResetPasswordConfirmation")</h4>
        </div>
        <div class="alert alert-success" role="alert">
            <p>
                Your password has been reset. Please <a asp-action="Login">click here to log in</a>.
            </p>
        </div>
    </div>
</div> 