@model ProfileVModel
   <!-- /.row -->
<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-header">
                <h5 class="card-title">@L("UserTracking")</h5> 
                <div class="card-tools">
                    <button type="button" class="btn btn-tool" data-card-widget="collapse">
                        <i class="fas fa-minus"></i>
                    </button>
                    <button type="button" class="btn btn-tool" data-card-widget="remove">
                        <i class="fas fa-times"></i>
                    </button>
                </div>
            </div>
            <!-- /.card-header -->
            <div class="card-body"> 
                <div class="row">
                    <div class="col-sm-3 col-md-3 col-lg-3">
                        <div id="profile-tracking-tabs" class="nav flex-column nav-tabs h-100" role="tablist" aria-orientation="vertical">
                            <a class="nav-link active" id="vert-tabs-Height-tab" data-toggle="pill" href="#vert-tabs-Height" data-bodyPart="Height" role="tab" aria-controls="vert-tabs-Height" aria-selected="true">@L("Height")</a>
                            <a class="nav-link" id="vert-tabs-Weight-tab" data-toggle="pill" href="#vert-tabs-Weight" data-bodyPart="Weight" role="tab" aria-controls="vert-tabs-Weight" aria-selected="false">@L("Weight")</a>
                            <a class="nav-link" id="vert-tabs-Hip-tab" data-toggle="pill" href="#vert-tabs-Hip" data-bodyPart="Hip" role="tab" aria-controls="vert-tabs-Hip" aria-selected="false">@L("Hip")</a>
                            <a class="nav-link" id="vert-tabs-BellyButtonWaist-tab" data-toggle="pill" href="#vert-tabs-BellyButtonWaist" data-bodyPart="BellyButtonWaist" role="tab" aria-controls="vert-tabs-BellyButtonWaist" aria-selected="false">@L("BellyButtonWaist")</a>
                            <a class="nav-link" id="vert-tabs-HipBoneWaist-tab" data-toggle="pill" href="#vert-tabs-HipBoneWaist" data-bodyPart="HipBoneWaist" role="tab" aria-controls="vert-tabs-HipBoneWaist" aria-selected="false">@L("HipBoneWaist")</a>
                            <a class="nav-link" id="vert-tabs-Chest-tab" data-toggle="pill" href="#vert-tabs-Chest" data-bodyPart="Chest" role="tab" aria-controls="vert-tabs-Chest" aria-selected="false">@L("Chest")</a>
                            <a class="nav-link" id="vert-tabs-RightArm-tab" data-toggle="pill" href="#vert-tabs-RightArm" data-bodyPart="RightArm" role="tab" aria-controls="vert-tabs-RightArm" aria-selected="false">@L("RightArm")</a>
                            <a class="nav-link" id="vert-tabs-LeftArm-tab" data-toggle="pill" href="#vert-tabs-LeftArm" data-bodyPart="LeftArm" role="tab" aria-controls="vert-tabs-LeftArm" aria-selected="false">@L("LeftArm")</a>
                            <a class="nav-link" id="vert-tabs-RightThigh-tab" data-toggle="pill" href="#vert-tabs-RightThigh" data-bodyPart="RightThigh" role="tab" aria-controls="vert-tabs-RightThigh" aria-selected="false">@L("RightThigh")</a>
                            <a class="nav-link" id="vert-tabs-LeftThigh-tab" data-toggle="pill" href="#vert-tabs-LeftThigh" data-bodyPart="LeftThigh" role="tab" aria-controls="vert-tabs-LeftThigh" aria-selected="false">@L("LeftThigh")</a>
                            <a class="nav-link" id="vert-tabs-RightCalve-tab" data-toggle="pill" href="#vert-tabs-RightCalve" data-bodyPart="RightCalve" role="tab" aria-controls="vert-tabs-RightCalve" aria-selected="false">@L("RightCalve")</a>
                            <a class="nav-link" id="vert-tabs-LeftCalve-tab" data-toggle="pill" href="#vert-tabs-LeftCalve" data-bodyPart="LeftCalve" role="tab" aria-controls="vert-tabs-LeftCalve" aria-selected="false">@L("LeftCalve")</a>
                            <a class="nav-link" id="vert-tabs-RightForeArm-tab" data-toggle="pill" href="#vert-tabs-RightForeArm" data-bodyPart="RightForeArm" role="tab" aria-controls="vert-tabs-RightForeArm" aria-selected="false">@L("RightForeArm")</a>
                            <a class="nav-link" id="vert-tabs-LeftForeArm-tab" data-toggle="pill" href="#vert-tabs-LeftForeArm" data-bodyPart="LeftForeArm" role="tab" aria-controls="vert-tabs-LeftForeArm" aria-selected="false">@L("LeftForeArm")</a>
                        </div>
                    </div>
                    <div class="col-sm-9 col-md-9 col-lg-9">
                        <div class="tab-content" id="vert-tabs-tabContent">
                            <div class="tab-pane text-left fade show active" id="vert-tabs-profile-body" role="tabpanel" aria-labelledby="vert-tabs-Height-tab">
                                @await Html.PartialAsync("_UserTrackingDetail", Model.UserTrackingFilter)
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /.row -->
            </div>
            <!-- ./card-body --> 
        </div>
        <!-- /.card -->
    </div>
    <!-- /.col -->
</div>